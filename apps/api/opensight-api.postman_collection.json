{
  "info": {
    "name": "OpenSight API",
    "description": "Complete API collection for OpenSight platform - AI-powered brand visibility monitoring.\n\nSetup:\n1. Register a user or login to get a JWT token\n2. The token is automatically saved to the `accessToken` collection variable\n3. All authenticated endpoints use the Bearer token from the variable\n\nBase URL: http://localhost:4000",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:4000",
      "type": "string"
    },
    {
      "key": "accessToken",
      "value": "",
      "type": "string"
    },
    {
      "key": "brandId",
      "value": "",
      "type": "string"
    },
    {
      "key": "promptId",
      "value": "",
      "type": "string"
    },
    {
      "key": "competitorId",
      "value": "",
      "type": "string"
    },
    {
      "key": "apiKeyId",
      "value": "",
      "type": "string"
    },
    {
      "key": "notificationId",
      "value": "",
      "type": "string"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{accessToken}}",
        "type": "string"
      }
    ]
  },
